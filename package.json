{
   "name": "@deebeetech/is-helper",
   "version": "1.2.0",
   "description": "A collection of \"is\"-style helpers to help solidify if something \"is\" something else. Has no dependencies and uses only raw JavaScript methodology.",
   "homepage": "https://github.com/deebee-tech/is-helper",
   "license": "MIT",
   "private": false,
   "author": {
      "name": "Sandy Weatherby",
      "email": "unicornbeachdiaries@gmail.com"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/deebee-tech/is-helper"
   },
   "type": "module",
   "main": "./dist/index.js",
   "exports": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
   },
   "types": "dist/index.d.ts",
   "publishConfig": {
      "access": "public"
   },
   "keywords": [
      "is",
      "helper",
      "type",
      "check",
      "checkers",
      "type-check",
      "type-checker",
      "type-checkers",
      "type-checking"
   ],
   "scripts": {
      "build": "rollup -c && cpy './package.json' './dist/' && cpy './README.md' './dist/' && cpy './CHANGELOG.md' './dist/'",
      "test": "vitest --coverage"
   },
   "devDependencies": {
      "@eslint/js": "^9.31.0",
      "@sebbo2002/semantic-release-jsr": "^3.0.0",
      "@semantic-release/changelog": "^6.0.3",
      "@semantic-release/commit-analyzer": "^13.0.1",
      "@semantic-release/git": "^10.0.1",
      "@semantic-release/github": "^11.0.3",
      "@semantic-release/npm": "^12.0.2",
      "@semantic-release/release-notes-generator": "^14.0.3",
      "@vitest/coverage-v8": "^3.2.4",
      "cpy-cli": "^5.0.0",
      "esbuild": "^0.25.6",
      "eslint": "^9.31.0",
      "eslint-config-prettier": "^10.1.5",
      "prettier": "^3.6.2",
      "rollup": "^4.45.1",
      "rollup-plugin-dts": "^6.2.1",
      "rollup-plugin-esbuild": "^6.2.1",
      "semantic-release": "^24.2.7",
      "semantic-release-replace-plugin": "^1.2.7",
      "typescript": "^5.8.3",
      "typescript-eslint": "^8.37.0",
      "vitest": "^3.2.4"
   }
}
